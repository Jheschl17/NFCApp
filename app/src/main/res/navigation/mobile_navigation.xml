<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="net.htlgrieskirchen.at.jeschl17.nfcdroid.ui.tags.TagsFragment"
        android:label="@string/title_profiles"
        tools:layout="@layout/fragment_Tags" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="net.htlgrieskirchen.at.jeschl17.nfcdroid.ui.scan.ScanFragment"
        android:label="@string/title_scan"
        tools:layout="@layout/fragment_scan" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="net.htlgrieskirchen.at.jeschl17.nfcdroid.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>
